-- // The Hero Tool Dance \\ --
local tool1 = IT("Tool", Backpack)
tool1.Name = "The Hero"
tool1.RequiresHandle = false
--
if not isfile("Epik Musics/The Hero.mp3") then
       writefile("Epik Musics/The Hero.mp3", game:HttpGet("https://github.com/gObl00x/Epik-Musics/raw/refs/heads/main/The%20Hero.mp3"))
end
local Hero = R6Emotes:FindFirstChild("The Hero")
--
tool1.Equipped:Connect(function()
    if Character then
    IsDancing = true
    getgenv().jumping = nil
    getgenv().moving = nil
    theme:Stop()
    Humanoid.WalkSpeed = 10
       getgenv().Animator6D(Hero, 1, true)
-- Music
Music1.Parent = Character
Music1.SoundId = getcustomasset("Epik Musics/The Hero.mp3")
Music1.Looped = true
Music1:Play()
end
end)

tool1.Unequipped:Connect(function()
if Music1.Parent == Character and Music1.Playing then
    IsDancing = false
    getgenv().Animator6DStop()
    theme:Play()
    Humanoid.WalkSpeed = 16
       Music1:Stop()
       Music1.Parent = EffectFolder
    end
    print("Dance 1 and music stopped")
end)


