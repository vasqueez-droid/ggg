-- // Cha Cha Tool Dance \\ --
local tool16 = IT("Tool", Backpack)
tool16.Name = "Cha Cha"
tool16.RequiresHandle = true
--
if not isfile("Epik Musics/Cha Cha.mp3") then
       writefile("Epik Musics/Cha Cha.mp3", game:HttpGet("https://github.com/gObl00x/Epik-Musics/raw/refs/heads/main/Cha%20Cha.mp3"))
end
local ChaCha = R6Emotes:FindFirstChild("Silly")
--
-- Handle
local handle = IT("Part")
handle.Name = "Handle"
handle.Size = V3(1, 4, 1)
handle.CanCollide = false
handle.Anchored = false
handle.Parent = tool16
-- Confetti
local conf = game:GetObjects("rbxassetid://688963157")[1]:FindFirstChildOfClass("ParticleEmitter"):Clone()
conf.Parent = EffectFolder
conf.Enabled = false
--
tool16.Equipped:Connect(function()
    if Character and maraca.Parent == EffectFolder and conf.Parent == EffectFolder then
    IsDancing = true
    getgenv().jumping = nil
    getgenv().moving = nil
    theme:Stop()
		maraca.Parent = handle
		conf.Parent = Head
		conf.Enabled = true
        Humanoid.WalkSpeed = 2.5
       getgenv().Animator6D(ChaCha, 1, true)
-- Music
Music16.Parent = Character
Music16.SoundId = getcustomasset("Epik Musics/Cha Cha.mp3")
Music16.Looped = true
Music16:Play()
end
end)

tool16.Unequipped:Connect(function()
if Music16.Parent == Character and Music16.Playing then
Humanoid.WalkSpeed = 16
    IsDancing = false
    getgenv().Animator6DStop()
    theme:Play()
    Music16:Stop()
    Music16.Parent = EffectFolder
end
if maraca.Parent == handle then
   maraca.Parent = EffectFolder
end
if conf and conf:IsA("ParticleEmitter") then
   conf.Enabled = false
   conf.Parent = EffectFolder
end
    print("Dance 16 and music stopped")
end)


