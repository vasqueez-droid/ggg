-- // Hakari Tool Dance \\ --
local tool22 = IT("Tool", Backpack)
tool22.Name = "Hakari"
tool22.RequiresHandle = false
--
if not isfile("Epik Musics/Hakari.mp3") then
       writefile("Epik Musics/Hakari.mp3", game:HttpGet("https://github.com/gObl00x/Epik-Musics/raw/refs/heads/main/Hakari.mp3"))
end
--
local CachedEffects = {}
tool22.Equipped:Connect(function()
    if Character then
    IsDancing = true
    getgenv().jumping = nil
    getgenv().moving = nil
    theme:Stop()
       getgenv().Animator6D(17147170140, 1, true)
-- Hakari Aura
local HakariAura = game:GetObjects("rbxassetid://17286931410")[1]
for _, limb in ipairs(Character:GetChildren()) do
	if limb:IsA("BasePart") then
for _, obj in ipairs(HakariAura:GetDescendants()) do
	if obj:IsA("ParticleEmitter") or obj:IsA("Trail") or obj:IsA("Beam") then
		local Aura = obj:Clone()
		Aura.Parent = limb
		table.insert(CachedEffects, Aura)
		   end
		end
	end
end
HakariAura:Destroy()
-- Music
Music22.Parent = Character
Music22.SoundId = getcustomasset("Epik Musics/Hakari.mp3")
Music22.Looped = true
Music22:Play()
end
end)

tool22.Unequipped:Connect(function()
if Music22.Parent == Character and Music22.Playing then
    IsDancing = false
    getgenv().Animator6DStop()
    theme:Play()
       Music22:Stop()
       Music22.Parent = EffectFolder
    end
    for _, Aura in pairs(CachedEffects) do
           Aura:Destroy()
		end
    table.clear(CachedEffects)
    print("Dance 22 and music stopped")
end)


